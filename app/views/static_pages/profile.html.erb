<!-- Modal Contact & Personal Information -->
<div class="modal fade" id="myModalContact" tabindex="-1" role="dialog" aria-labelledby="myModalLabelContact">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabelContact">Contact and Personal Information</h4>
      </div>
      <div class="modal-body">
        <%= form_tag(update_user_user_details_path, :method=>'post') do %>
        <div class="form-group"> 
            <%= label_tag 'addr', 'Address' %>
            <%= text_field_tag 'addr', current_user.addr, class: "form-control" %>
        </div>
        <div class="form-group"> 
            <%= label_tag 'phone', 'Phone' %>
            <%= text_field_tag 'phone', current_user.phone, class: "form-control" %>
        </div>
        <div class="form-group"> 
            <%= label_tag 'gender', 'Gender' %>
            <%= text_field_tag 'gender', current_user.gender, class: "form-control" %>
        </div>
        <div class="form-group"> 
            <%= label_tag 'status', 'Status' %>
